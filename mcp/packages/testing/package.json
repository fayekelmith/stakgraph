{
  "name": "@stakgraph/testing",
  "version": "1.0.0",
  "type": "module",
  "description": "Stakgraph testing utilities and Playwright test generation",
  "main": "server.js",
  "scripts": {
    "build": "echo \"No build needed - using JS files\"",
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}